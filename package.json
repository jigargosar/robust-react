{
  "name": "robust-react",
  "version": "0.0.0-development",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@cerebral/react": "3.2.1",
    "@material-ui/icons": "1.0.0-beta.43",
    "cerebral": "4.2.1",
    "firebaseui": "2.7.0",
    "hyperscript": "2.0.2",
    "hyperscript-helpers": "3.0.3",
    "jss": "9.8.1",
    "libx": "1.0.0-alpha.0",
    "material-ui": "1.0.0-beta.41",
    "mobx": "4.1.1",
    "mobx-react": "5.0.0",
    "mobx-state-tree": "2.0.4",
    "nanoid": "1.0.2",
    "ramda": "0.25.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-hyperscript": "3.2.0",
    "roboto-fontface": "0.9.0",
    "sanctuary": "0.14.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-console": "1.0.0",
    "@storybook/addon-jest": "3.4.2",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addon-options": "3.4.2",
    "@storybook/addon-storysource": "3.4.3",
    "@storybook/addons": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-plugin-espower": "2.4.0",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-runtime": "^6.26.0",
    "chance": "1.0.13",
    "dom-testing-library": "1.9.0",
    "enzyme": "3.3.0",
    "expect": "22.4.3",
    "flow-bin": "0.70.0",
    "flow-runtime": "0.17.0",
    "foreman": "2.0.0",
    "jest": "22.4.3",
    "jest-mock": "22.4.3",
    "jest-runner-eslint": "0.4.0",
    "jest-runner-flowtype": "0.0.3",
    "jest-runner-prettier": "0.2.2",
    "jest-skipped-reporter": "0.0.4",
    "jss-snapshot-serializer": "1.0.0",
    "kcd-scripts": "0.37.0",
    "metalsmith": "2.3.0",
    "mocha": "5.1.1",
    "nightmare": "3.0.1",
    "nodemon": "1.17.3",
    "power-assert": "1.5.0",
    "prop-types": "15.6.1",
    "react-jss": "8.4.0",
    "react-scripts": "1.1.4",
    "react-testing-library": "2.1.1",
    "storybook-addon-specifications": "2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/kcd-scripts/eslint.js",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "react/prop-types": "on",
      "import/no-unassigned-import": "on",
      "babel/new-cap": "off",
      "no-duplicate-imports": "off",
      "import/prefer-default-export": "off",
      "import/no-duplicates": "error"
    }
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": false,
      "format": "codeframe"
    }
  },
  "scripts": {
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "start": "react-scripts start",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s",
    "precommit": "kcd-scripts precommit",
    "postcommit": "git update-index -g",
    "build:react": "react-scripts build",
    "test:react-scripts": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p $PORT$ -s public",
    "build-storybook": "build-storybook -s public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jigargosar/robust-react.git"
  }
}
